{
  "name": "cs-rat",
  "version": "1.0.0",
  "description": "Blank electron template",
  "main": "main.js",
  "author": {
    "name": "Anthonym",
    "email": "samuelmatheson20@gmail.com"
  },
  "homepage": "https://null.none",
  "repository": {
    "type": "git",
    "url": "https://github.com"
  },
  "icon": "assets/icons/icon.png",
  "license": "ISC",
  "bin": "BB01",
  "scripts": {
    "start": "electron main.js",
    "deb-linux": "electron-installer-debian --src dist/{output-folder}/ --arch x64 --config debian.json",
    "postinstall": "electron-builder install-app-deps",
    "builder-linux": "electron-builder --linux deb appimage tar.xz snap",
    "builder-win": "electron-builder --win --x64 --ia32"
  },
  "build": {
    "appId": "com.cs.rat",
    "icon": "assets/icons/icon.png",
    "productName": "blank electron template",
    "copyright": "Copyright © 2020 ${author}",
    "buildVersion": "1.0.0",
    "remoteBuild": "true",
    "asar": "true",
    "win": {
      "target": "nsis",
      "icon": "assets/icons/icon.ico"
    },
    "linux": {
      "target": "deb",
      "category": "utility",
      "icon": "assets/icons/icon.png"
    }
  },
  "devDependencies": {
    "asar": "^1.0.0",
    "electron": "^10.1.5",
    "electron-builder": "^22.9.1"
  },
  "dependencies": {
    "electron-store": "^6.0.1",
    "electron-window-state": "^5.0.3"
  }
}
